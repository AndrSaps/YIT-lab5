@{
    ViewData["Title"] = "Factories";
}
@model Milkify.Core.Datatable.DatatableRequests.DatatableResult

<div class="card">
    <div class="card-header"><h5>Factories</h5></div>
    <div class="card-body">
        <div class="row mb-2">
            <div class="col-12">
                <a class="btn btn-primary ml-2" asp-action="Create">Create Factory</a>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                @await Html.PartialAsync("~/Views/Common/Datatable.cshtml", @Model)
            </div>
        </div>
       
    </div>
</div>
